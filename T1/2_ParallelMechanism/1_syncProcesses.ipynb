      "source": [
        "from multiprocessing import Process, Lock\n",
        "\n",
        "def f(l, i):\n",
        "    l.acquire()\n",
        "    print 'hello world', i\n",
        "    l.release()\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    lock = Lock()\n",
        "\n",
        "    for num in range(10):\n",
        "        Process(target=f, args=(lock, num)).start()"
      ]
    }
  ],
